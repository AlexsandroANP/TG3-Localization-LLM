		LocDirectEntry
		{
			Key = "$ui.GuidedTutorial.Produce2";
			Text = "Double-cliquez sur votre entreprise ou sélectionnez le bâtiment, puis sélectionnez l’action Ouvrir le menu Bâtiment dans la barre d’actions.";
		},
		LocDirectEntry
		{
			Key = "$ui.GuidedTutorial.Produce3";
			Text = "Recruter un transporteur";
		},
		LocDirectEntry
		{
			Key = "$ui.GuidedTutorial.Produce4";
			Text = "Cliquez sur Recruter des employés dans la section Transport du menu Production.";
		},
		LocDirectEntry
		{
			Key = "$ui.GuidedTutorial.Produce5";
			Text = "Recruter des ouvriers";
		},
		LocDirectEntry
		{
