		LocDirectEntry
		{
			Key = "$randomEvents.negative.7";
			Text = "Ihr müsst %coins% an Kirchenzehntgeldern nachzahlen.";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.negative.8";
			Text = "Ein seriös wirkender Außendienst-Fernhändler überredet Euch in einer schwachen Stunde zum Erwerb von Länderreien im kürzlich wieder aus den Fluten aufgetauchten Atlantis.\nIhr erhaltet 13 1/2 Hektar zum Vorzugspreis von nur %coins%...";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.negative.9";
			Text = "Zu spät bemerkt Ihr, dass Ihr Opfer eines Taschendiebstahl geworden seid. Euch fehlt ein Geldbeutel mit %coins% Talern.";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.negative.narration.1";
			Text = "Ihr seid ein öffentliches Ärgernis!";
		},
		LocDirectEntry
		{
