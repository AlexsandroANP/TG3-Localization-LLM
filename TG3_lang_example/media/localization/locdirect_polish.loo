		{
			Key = "$gui.Erect.GroupName.Rogue";
			Text = "Nielegalna działalność";
		},
		LocDirectEntry
		{
			Key = "$gui.Erect.GroupName.Other";
			Text = "Inne";
		},
		LocDirectEntry
		{
			Key = "$gui.Erect.GroupName.City";
			Text = "Miasto";
		},
		LocDirectEntry
		{
			Key = "$Robber";
			Text = "Rozbójnik";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1400.0";
			Text = "Były król Ryszard II zmarł jako więzień w zamku Pontefract. To znak od Boga, że rzeczywiście był słaby i że zdetronizowano go nie bez przyczyny.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1400.1";
			Text = "Przebywający na północy Walii król Henryk IV zostaje zaatakowany przez Rhysa Ddu, ale naszemu wspaniałemu królowi udaje się sprawić, że walijski bunt nie przenosi się na południe.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1401.0";
			Text = "Bizancjum uratowane! Mongolski książę Timur Chromy uratował miasto, które przez pięć lat było oblegane przez Turków i pojmał sułtana Bajazyda I.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1401.1";
			Text = "W bitwie pod Brescią Visconti z Mediolanu pokonali znacznie mniej liczne siły Ruprechta III. Florencja obiecywała wsparcie dla króla, lecz były to obietnice wymuszone.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1402.0";
			Text = "Walijski nacjonalista, Owen Glendower, pokonuje i pojmuje anglo-normańskiego hrabiego, Edmunda Mortimera, którego ziemie leżą na granicy z Walią. Król Henryk nie rezygnuje i ciągle walczy przeciw walijskim buntownikom.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1402.1";
			Text = "Hrabia Henry Percy wraz z arystokratami z północy odpierają pod Homildon Hill armię szkocką dowodzoną przez hrabiego Douglas - ciągle trwa waśń między rodami Douglas i Percy.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1403.0";
			Text = "Konfederacja Szwajcarska stoczyła z książętami Mediolanu wiele zaciętych bitew o ziemie w północnych Włoszech. Szwajcarom udało się w ten sposób powiększyć swe terytorium aż do Ticino.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1403.1";
			Text = "Król Henryk IV pokonuje Henry’ego %\"Hotspura%\" Percy’ego w bitwie pod Shrewsbury. Mówi się, że buntownik %\"Hotspur%\" ginie z ręki króla. Papież Benedykt XIII ucieka z Avignon, gdzie był oblegany przez Francuzów. Później Francja ponownie przysięga posłuszeństwo rządzonemu przezeń Kościołowi.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1404.0";
			Text = "Rada Republiki ogłasza, iż miasta Vicenza oraz Belluno dobrowolnie poddały się władzy doży.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1404.1";
			Text = "Przynoszę wieści z bardzo daleka - z kraju znanego z przypraw, bogactwa, ale również z barbarzyństwa. Przywódca Mongołów, Timur Chromy zniszczył potęgę Złotej Ordy na Rusi. Mam jeszcze ostrzeżenie dla grasujących w pobliżu czarnoksiężników i wiedźm: parlament przegłosował ustawę zakazującą alchemii, żeby zabronić produkcji srebra i złota.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1405.0";
			Text = "Dobry to rok dla Republiki! Oddziałom weneckim udało się wyrwać Weronę ze szponów Mediolanu. Viscontim się to nie spodoba...";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1405.1";
			Text = "Zmarł Timur Chromy. Czyżby zabił się sam? A może stało się to z pomocą wroga, albo własnych ludzi? Nigdy się nie dowiemy. Pewne jest jedynie to, że stworzył potężne imperium i podbił wiele narodów. Poszukiwacze przygód twierdzą, że ten szalony władca zbierał czaszki wrogów - dzięki Bogu, że nie dotarł do naszych wybrzeży.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1406.0";
			Text = "Podczas gdy Florencja łapczywie wchłonęła Pizę, Wenecja przyłączyła Padwę do terytorium Republiki.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1406.1";
			Text = "Papież Innocenty ekskomunikuje króla Władysława z Neapolu i oficjalnie pozbawia go królestwa. Zastanawiacie się czasem, jak to jest w Rzymie? Dziś wieczorem wędrowni artyści zaprezentują tańce rodem z Rzymu, więc możecie się przekonać.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1407.0";
			Text = "Francuzi walczą między sobą - zamordowano Ludwika Orleańskiego i rozpoczęła się wojna między armaniakami i burgundczykami.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1407.1";
			Text = "Uwaga, w nadchodzących tygodniach odbędą się egzekucje heretyków. Wierni postanowili, iż trzeba uśmiercić wiedźmy i czarowników, którzy dręczą nasze święte ziemie.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1408.0";
			Text = "Dziś, 10 kwietnia, zmarła Elizabeth Despenser, córka barona le Despencer. W opactwie Tewkesbury w hrabstwie Gloucestershire odbędzie się nabożeństwo żałobne.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1408.1";
			Text = "Syn króla, Henryk, prowadzi do Walii armię, która ma odebrać Aberystwyth buntownikowi, Owenowi Glendowerowi. Odwraca się bieg wojny i teraz to walijscy buntownicy uciekają.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1409.0";
			Text = "Z Pizy dobiegają niesamowite wieści! Tamtejszy sobór chce obalić papieża oraz antypapieża i uczynić Aleksandra V nową głową Kościoła.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1409.1";
			Text = "Na soborze w Pizie kardynałowie usiłują rozmawiać o ciężkiej sytuacji Kościoła, o władzę nad którym walczą trzej papieże. Władysław Wspaniałomyślny zostaje wypędzony z Rzymu po tym, jak rusza na miasto, chcąc zaprotestować przeciw nałożonej na siebie ekskomunice.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1410.0";
			Text = "Szaleństwa w Pizie ciąg dalszy! Sobór wybrał Aleksandra V na papieża.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1410.1";
			Text = "Henryk IV odbił Walię z rąk resztek buntowników. Teraz trwają poszukiwania Owena Glendowera i jego zwolenników.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1411.0";
			Text = "Po długiej nieobecności spowodowanej walką o tron król Henryk IV przejął władzę od swego syna, Henryka V. Miejmy nadzieję, że w najbliższej przyszłości nie będzie już więcej wojen.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1412.0";
			Text = "Umiera Małgorzata I, królowa Danii, Norwegii i Szwecji. Ponoć była dobrą królową i biskup prosi o chwilę ciszy w jej intencji. Swe rządy rozpoczyna Eryk Pomorski, niech Pan obdarzy go łaskami.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1413.0";
			Text = "Z kraju Turków dobiegły nowe wieści dotyczące sułtana Mehmeda I. Mianował się następcą zmarłego mongolskiego księcia Timura Chromego i wkrótce może zagrozić chrześcijańskiemu Zachodowi!";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1413.1";
			Text = "Król Neapolu, Władysław Wspaniałomyślny ponownie zajmuje Rzym, zmuszając papieża do ucieczki. Biskup odprawi specjalną mszę w intencji papieża i potępienia jego przeciwnika.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1414.0";
			Text = "Henryk V zgłasza pretensje do tronu francuskiego, tak jak zrobił to Edward III. Sir John Oldcastle dowodzi buntem lollardów, który kompromituje ruch, łącząc go z radykalizmem społecznym.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1414.1";
			Text = "Zmarł Władysław Wspaniałomyślny, król Neapolu - tron po nim przejęła jego siostra, Joanna II. To oczywiste, iż ten czarownik zginął rażony gromem z nieba.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1415.0";
			Text = "Sobór w Konstancji ogłosił, iż decyzje soborów są ważniejsze od decyzji papieży. Ciekawe, czy niedługo zaczną samemu Bogu rozkazywać?!";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1415.1";
			Text = "Arystokracja w Czechach protestuje przeciwko zapowiadanej egzekucji reformatora Jana Husa, który został oskarżony o herezję na soborze w Konstancji po tym, jak odmówił potępienia pism Johna Wyclifa. W lipcu Jan Hus zostaje spalony na stosie, a dwa dni później papież Grzegorz XII oficjalnie kończy sobór w Konstancji.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1416.0";
			Text = "Radujcie się, zacni mieszkańcy! Sułtan Mehmed I rzucił wyzwanie Republice, lecz teraz jego dumna flota spoczywa na dnie morza nieopodal półwyspu Gallipoli.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1416.1";
			Text = "Król Ferdynand I Aragoński wydał ostatnie tchnienie.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1417.0";
			Text = "Nareszcie! Sobór w Konstancji nakłonił wszystkich trzech papieży do abdykacji i tak oto zakończyła się wielka schizma. Na Tronie Piotrowym zasiada teraz papież Marcin V.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1417.1";
			Text = "Sir Henry Barton, burmistrz Londynu, rozkazał zawieszać na ulicach latarnie w zimowe wieczory pomiędzy dniem Wszystkich Świętych i świętem Matki Boskiej Gromnicznej. Oświetlenie uliczne zyskuje na popularności również w innych częściach Anglii.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1418.0";
			Text = "Nowe wieści dotarły do nas z Florencji. Mistrz Filippo Brunelleschi rozpoczął budowę olbrzymiej kopuły dla tamtejszej katedry.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1418.1";
			Text = "Angielski przywódca lollardów, John Oldcastle, zostaje uznany za zdrajcę przez Henryka V i spalony za herezję.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1419.0";
			Text = "Radujcie się, dobrzy obywatele! Na weneckim Canal Grande rozpoczęto budowę pierwszego wspaniałego pałacu. Znak to wielkiego dobrobytu w naszej Republice.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1419.1";
			Text = "Jan Nieustraszony, książę Burgundii, zostaje zamordowany, a jego śmierć zostaje przypisana zwolennikom delfina Francji.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1420.0";
			Text = "Dotarły do nas wieści z drugiej strony Alp! Papież Marcin V ogłosił krucjatę przeciw husytom, którzy wszczęli powstanie na wschodzie Europy i obecnie pustoszą ziemie niemieckie. Ci kacerze mordują każdego, kto nie pierzcha dość szybko!";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1420.1";
			Text = "W Czechach rozpoczęły się wojny husyckie, gdy cesarz Zygmunt poniósł porażkę w bitwie pod Wyszehradem, która stanowiła część krucjaty przeciw heretykom.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1421.0";
			Text = "Ród Medyceuszów przejął władzę we Florencji. Mogą oni stanowić poważne zagrożenie dla naszej Republiki.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1421.1";
			Text = "Dobrzy ludzie, przynoszę wieści z daleka! Zmarł sułtan turecki Mehmed I, a władzę po nim obejmuje jego syn Murad II.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1422.0";
			Text = "Turcy osmańscy oblegają Konstantynopol. Doża przyjmuje wysłanników Bizancjum, którzy to proszą o wspomożenie przeciw Turkom.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1422.1";
			Text = "Zmarł król Francji, Karol VI, a król Henryk VI zajął jego miejsce. Niech żyje nowy król! Odbędzie się wspaniała uroczystość na jego cześć!";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1423.0";
			Text = "Bizancjum odda Wenecji Tessalonikę w zamian za odsiecz dla obleganego przez Turków Konstantynopola. Ta zbrojna pomoc nareszcie zmusi Turków do odwrotu.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1423.1";
			Text = "Richard Wittington, burmistrz Londynu odszedł od nas na zawsze. W kościele odbędzie się msza za jego duszę.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1424.0";
			Text = "Radujcie się, ludzie! Nasza Republika to największa potęga morska i handlowa na całym świecie. W samej Wenecji mieszka ponad dwieście tysięcy obywateli, a w porcie gotowych do bitwy mamy ponad pięćdziesiąt nowoczesnych galer!";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1424.1";
			Text = "Humphrey, książę Gloucester rozpoczyna kampanię mającą na celu odbicie ziem swej żony, Jacqueline, hrabiny Holandii, od Filipa, księcia Burgundii.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1425.0";
			Text = "Poślubiając nadobną księżniczkę, król Aragonii w końcu zdołał przyłączyć królestwo Nawarry do swych ziem.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1425.1";
			Text = "Humphrey, książę Gloucester, porzuca żonę dla Eleanor Cobham. Chyba zmęczyła go ciągła walka o małżonkę...";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1426.0";
			Text = "Ponoć husyccy kacerze z zimną krwią wymordowali saską odsiecz. Panie, świeć nad ich duszami.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1426.1";
			Text = "Bitwa pod St. James zmusza księcia Bretanii do uznania Anglii za swego suwerena.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1427.0";
			Text = "Czwarta krucjata przeciw husytom zakończyła się wraz z klęską pod Tachowem. Świeć Panie nad duszami tych dobrych chrześcijan.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1427.1";
			Text = "Wiedźmy, miejcie się na baczności, rozpoczęło się polowanie na czarownice!";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1428.0";
			Text = "Florencja opłakuje wspaniałego patrycjusza, Giovanniego de Medici, któremu wielu wenecjan zawdzięcza swe fortuny.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1428.1";
			Text = "Siły angielskie dowodzone przez Tomasza Montague, hrabiego Salisbury, rozpoczynają oblężenie Orleanu.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1429.0";
			Text = "Portugalscy żeglarze założyli placówkę handlową na niedawno odkrytych Azorach. Nie znajdą jednak żadnych bogactw tak blisko krańca świata.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1429.1";
			Text = "Czy mnie uszy nie mylą? Ta Joanna d'Arc poprowadziła Francuzów do zwycięstwa nad wojskami angielskimi dowodzonymi przez Talbota. Przerwano oblężenie Orleanu, co wlało w serca Francuzów falę uczuć patriotycznych.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1430.0";
			Text = "Hańba! Doża oznajmił dziś, iż nasze posiadłości w Tessalonice wpadły w łapska chciwych Osmanów. Czarny to dzień dla Wenecji.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1430.1";
			Text = "Turcy zajmują należące do Republiki Weneckiej Saloniki i ciągle rosną w siłę, a cesarstwo rzymskie traci punkt oparcia.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1431.0";
			Text = "Biały dym obwieścił, iż rzymskie konklawe dokonało wyboru nowego papieża. Oby Eugeniusz IV długo dzierżył pastorał.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1431.1";
			Text = "Kobieta, która twierdziła, że jest posłannicą Boga, okazała się demonicą z piekła rodem i spłonęła na stosie oskarżona o czary. Joanna d'Arc nie żyje!!!";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1432.0";
			Text = "Niech Bóg zmiłuje się nad mieszkańcami Konstantynopola. Osmanowie ponownie chwycili miasto w żelazny uścisk.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1432.1";
			Text = "W Saksonii, na Śląsku i w innych częściach Niemiec wybuchają powstania chłopskie.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1433.0";
			Text = "Bez konkretnej przyczyny cesarz Chin zerwał kontakty handlowe z innymi krajami, niwecząc tym samym wszystko, co swą wyprawą osiągnął Marco Polo.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1433.1";
			Text = "Wieści od podróżników - dynastia Ming zlikwidowała swą flotę na Oceanie Indyjskim. To może oznaczać początek panowania zachodnich flot, a Portugalia jako pierwsza wprowadza w życie taki pomysł na podboje.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1434.0";
			Text = "Cosimo de Medici dokonał przewrotu i przejął władzę we Florencji. Doża z pewnością będzie miał na niego baczne oko.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1434.1";
			Text = "Wojsko angielskie tłumi bunty chłopskie w Normandii.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1435.0";
			Text = "Filip II i król Francji Karol VII podpisują pokój w Arras, kładąc w ten sposób kres sojuszowi Burgundii z Anglią.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1435.1";
			Text = "Umiera ladacznica Joanna Andegaweńska, a René I otrzymuje królestwo Neapolu.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1436.0";
			Text = "Papież Eugeniusz IV osobiście wyświęci nową katedrę we Florencji, Santa Maria del Fiore. Poślednia to budowla, której żaden wenecjanin ni chwili uwagi by nie poświęcił.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1436.1";
			Text = "Henryk VI osiąga pełnoletniość i zaczyna osobiście rządzić Anglią. Niech żyje król! Hurra!";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1437.0";
			Text = "Bizantyjski cesarz Jan VIII wyrusza do Rzymu, by szukać pomocy przeciw rosnącemu w siłę imperium osmańskiemu.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1437.1";
			Text = "Umiera król Szkocji Jakub I - to kara za ciągłe ataki na północy Anglii.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1438.0";
			Text = "Portugalczycy opłakują śmierć króla Edwarda. Jego syn i następca, Alfons V, zasiądzie na tronie, gdy osiągnie pełnoletność.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1438.1";
			Text = "Przybądźcie wieczorem do katedry, gdzie biskup odprawi mszę za Annę z Gloucester - arystokratkę, która urodziła się jako anioł i oddała się Bogu.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1439.0";
			Text = "Zniechęcone i wystraszone przedstawicielstwo Kościoła Greckiego prowadzone cesarza bizantyjskiego Jana VIII przystaje na żądania papieża Eugeniusza. Dzięki zjednoczeniu Kościołów prawosławni uznają papieża za głowę całego świata chrześcijańskiego. Zdają sobie sprawę, że w 1422 papież Marcin V stwierdził, iż po zjednoczeniu wszystkich chrześcijan Turcy będą się bali atakować, a inni chrześcijanie chętniej pospieszą z pomocą. Prawosławni wiedzą, że teraz jedyną ich szansą są katolicy.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1439.1";
			Text = "Plymouth w Anglii to pierwsze miasto, które otrzymuje od parlamentu kartę przywilejów.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1440.0";
			Text = "Ponoć w Cesarstwie Niemieckim pewien mistrz rzemieślniczy zdołał powielić księgi bez użycia scriptorium. Czas pokaże, czy ten wynalazek komukolwiek się przyda.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1440.1";
			Text = "Czarnoksiężnik i morderca dzieci spalony! Jeden z najbardziej szanowanych i kochanych rycerzy francuskich został stracony w Nantes. Skazany za morderstwa przynajmniej dwustu dzieci, herezję, sodomię, apostazję, świętokradztwo i pogwałcenie nietykalności duchowieństwa Gilles de Rais został uduszony i spalony wraz z dwoma współsprawcami tych potwornych zbrodni.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1441.0";
			Text = "Biskup przynosi wieści z Rzymu - dwóch Etiopczyków bierze udział w soborze we Florencji stanowiącym część negocjacji dotyczących ewentualnej unii chrześcijaństwa koptyjskiego i rzymskokatolickiego.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1441.1";
			Text = "Król Henryk VI zakłada King's College na Uniwersytecie w Cambridge.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1442.0";
			Text = "Papież Eugeniusz IV nawołuje do krucjaty przeciw Turkom, jednak niewiele państw chrześcijańskich może usłuchać jego wezwania ze względu na wojny, które toczą między sobą. Turcy nieprzerwanie powiększają swe imperium.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1442.1";
			Text = "Ryszard, książę Yorku, z radością ogłasza narodziny swego syna, Edwarda.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1443.0";
			Text = "Albański książę Skanderbeg zadał dotkliwy cios dawnym panom i odłączył swe ziemie od imperium osmańskiego. Oby dobry Bóg sprowadził jeszcze więcej takich waśni na te hordy dzikusów.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1443.1";
			Text = "Jan Hunyadi pokonuje Turków w bitwie pod Nisz.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1444.0";
			Text = "Turecka krucjata Jánosa Hunyadyego poniosła klęskę. Chrześcijanie zostali pobici przez muzułmanów w krwawej potyczce nieopodal miasta Warna.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1444.1";
			Text = "Armie Polski i Węgier zostają zmiażdżone w bitwie pod Warną przez Turków dowodzonych przez Murada II.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1445.0";
			Text = "Portugalscy kupcy założyli placówkę handlową w Mauretanii.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1445.1";
			Text = "Zmarła Joanna Beaufort, królowa Szkocji.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1446.0";
			Text = "Nowo powstałe oddziały janczarów pomagają Mehmedowi II zmusić swego ojca, Murada II do abdykacji.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1446.1";
			Text = "Turecki władca Murad II najeżdża Moreę - wydaje się, że mimo krucjat nie da się go powstrzymać.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1447.0";
			Text = "Wiwat, mamy nowego papieża! Pod bożym przewodnictwem konklawe wybrało papieżem Mikołaja V.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1447.1";
			Text = "Zmarł Lord Kanclerz, Humphrey, ale przecież król sobie poradzi, nieprawdaż? Naturalnie, że tak.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1448.0";
			Text = "Trwa bisurmańska plaga. Dumne serbskie miasto Nisz wpadło w plugawe łapska niewiernych.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1448.1";
			Text = "Umiera Jan VIII Paleolog, cesarz bizantyjski, a Turcy pokonują kolejną armię krzyżowców i otaczają Konstantynopol. Czyżby nadchodził zmierzch cesarstwa?";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1449.0";
			Text = "Konstantyn XI zostaje cesarzem bizantyjskim.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1449.1";
			Text = "Koniec pokoju między Anglia a Francją - siły francuskie przejmują Rouen.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1450.0";
			Text = "W Mediolanie dochodzi do zmiany warty. Koniec władania rodu Viscontich - rządy nad miastem-państwem obejmują Sforzowie.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1450.1";
			Text = "Unia kalmarska - Dania, Norwegia i Szwecja decydują, kto zostanie ich królem.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1451.0";
			Text = "Umiera sułtan Murad II, a władzę przejmuje jego syn, Mehmed II. Miejmy nadzieję, iż nie odziedziczył diabelskich zdolności ojca.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1451.1";
			Text = "Umiera sułtan Murad II, jego następcą zostaje Mehmed II - miejmy nadzieję, że oznacza to koniec tego heretyckiego imperium.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1452.0";
			Text = "Nad Konstantynopolem ponownie zgromadziły się ciemne chmury. Zwiadowcy donoszą, iż planowany jest kolejny szturm na miasto.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1452.1";
			Text = "Narodziny Ferdynanda II z Aragonii i doniesienia o portugalskich wyczynach na wybrzeżach Afryki. Idźcie na nabrzeże, jeśli chcecie zaznać przygód w dalekich krajach.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1453.0";
			Text = "Świat chrześcijański wstrzymał oddech! Konstantynopol, perła wschodu, wpadł w ręce muzułmanów. Ocalali donoszą o potwornych zbrodniach. Panie, zmiłuj się nad ich duszami.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1453.1";
			Text = "Francja odzyskuje Bordeaux. Dobiega końca wojna stuletnia pomiędzy Anglią i Francją. W rękach angielskich pozostaje tylko Calais.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1454.0";
			Text = "Nasz doża z dumą ogłasza, iż Wenecja zawarła z sułtanem Mehmedem II porozumienie handlowe.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1454.1";
			Text = "Pokój zawarty w Lodi położył kres walkom między Wenecją a rządzonym przez ród Sforzów Mediolanem.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1455.0";
			Text = "W walkach, które zaczęły być nazywane Wojną Dwóch Róż, Ryszardowi, księciu Yorku zostaje odebrany tytuł lorda protektora. Doprowadza to do otwartego sporu o tron pomiędzy dynastiami Lancaster i York. Król Henryk VI zostaje schwytany przez Yorkistów.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1455.1";
			Text = "Papież Mikołaj V wydał ostatnie tchnienie.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1456.0";
			Text = "Po krwawym i zaciekłym boju dzielni krzyżowcy zdziesiątkowali wojska Mehmeda II pod Belgradem i zmusili Osmanów do odwrotu.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1456.1";
			Text = "Wład Palownik zabija Władysława II Dana i przejmuje władzę na Wołoszczyźnie. Na pobliskich Węgrzech Jan Hunyadi pokonuję armię tureckiego sułtana Mehmeda II.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1457.0";
			Text = "Stefan III zabezpiecza tron mołdawski - jego lud i arystokracja uważa go za bardzo dobrego władcę.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1457.1";
			Text = "Maciej Korwin zostaje królem Węgier i kontynuuje walkę z Turkami.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1458.0";
			Text = "Umiera Kalikst III, a Pius II zostaje nowym papieżem. Niech żyje papież!";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1458.1";
			Text = "Utworzenie Magdalen College na Uniwersytecie w Oksfordzie.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1459.0";
			Text = "Mnich Fra Mauro w cudowny sposób połączył naukę ze sztuką, sporządzając mapę całego świata na życzenie samego doży.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1459.1";
			Text = "Poselstwa rozpuszczają plotki o okropnościach, które dzieją się za sprawą Turków i księcia na Wołoszczyźnie. Wład Palownik (gdyż tak jest teraz zwany) kazał wbić na pal tysiące żołnierzy wrogich armii, a nawet własnych ludzi.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1460.0";
			Text = "Łowcy przygód donoszą, że portugalscy żeglarze dotarli jeszcze dalej na południe Afryki i okrążyli Sierra Leone.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1460.1";
			Text = "Richard Neville, Hrabia Warwick i Marchii, ląduje w Anglii i siłą zajmuje Londyn. Miesiąc później gromi armię Lancasterów i pojmuje króla Henryka, którego następcą zostaje przedstawiciel rodu Yorków.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1461.0";
			Text = "Wraz z kapitulacją cesarstwa Trapezuntu całość niegdyś potężnego Bizancjum znalazła się w posiadaniu imperium osmańskiego.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1461.1";
			Text = "Rozgrywa się krwawa bitwa pod Towton - królowa Małgorzata zostaje pokonana przez Edwarda IV, który koronuje się na nowego króla Anglii. Podobno w ciągu zaledwie jednego dnia zginęło tu 30 000 żołnierzy.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1462.0";
			Text = "Wład Palownik zostaje zdetronizowany przez Mehmeda II na Wołoszczyźnie.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1462.1";
			Text = "Iwan III zastępuje na tronie Wasyla.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1463.0";
			Text = "Bośnia zdobyta! Kolejne z królestw pada łupem tureckiej nawałnicy. Kiedyż wreszcie nasycą oni swój głód podbojów?";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1464.0";
			Text = "Bitwa pod Hexham - armia Lancasterów ponownie pokonuje Małgorzatę, która wycofuje się do Francji.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1464.1";
			Text = "Zmarł papież Pius II! Świat czeka na nowego papieża.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1465.0";
			Text = "Od tej pory Tarent stanowi część Królestwa Neapolu. W ten oto sposób Neapol pragnie stanąć w szranki z naszą ukochaną Republiką. Cóż za próżny trud!";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1465.1";
			Text = "Król Henryk VI został pochwycony i uwięziony w londyńskiej Tower. Kto jest teraz naszym królem? Jednak posłuchajcie mnie - nie bójcie się, gdyż mamy nowego papieża, Pawła II, który pomoże nam w tych czasach pełnych waśni.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1466.0";
			Text = "Wieści z daleka - zakończyła się wojna trzynastoletnia pomiędzy Prusami, Polską i Zakonem Krzyżackim. Mieszkańcy Florencji opłakują niedawno zmarłego sławnego artystę, Donatella.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1466.1";
			Text = "Spragniony? W takim razie pędź do oberży i zamów piwo! Właściciel opuszcza cenę o połowę wszystkim, którzy pomogą mu wyłapać gryzonie w piwnicy.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1467.0";
			Text = "Turcy przejmują kontrolę nad Albanią i próbują uczynić to samo z Bośnią - rozprzestrzeniają się po Europie jak zaraza.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1467.1";
			Text = "Król Maciej Korwin zakłada pierwszy uniwersytet na Słowacji.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1468.0";
			Text = "Umiera angielska arystokratka Eleanor Talbot.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1468.1";
			Text = "W Niemczech umiera w spokoju Johannes Gutenberg - wynaleziony przez niego druk stał się jedną z ważniejszych przyczyn nastania zmian w Europie.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1469.0";
			Text = "Władzę nad Florencją przejął Lorenzo de Medici. Zobaczymy, jaką politykę ten nowy despota przyjmie wobec naszej Republiki.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1469.1";
			Text = "Lorenzo de Medici dochodzi do władzy we Florencji.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1470.0";
			Text = "Pod Lose-cote Field armata Edwarda IV zasiała panikę wśród buntowników z hrabstwa Lincolnshire.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1470.1";
			Text = "Przynoszę dobre wieści - król Henryk został przywrócony na tron Anglii. Bóg jest po naszej stronie!";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1471.0";
			Text = "Biada zwyciężonym. Wraz z upadkiem twierdzy Počitelj cała środkowa Bośnia stanęła otworem przed Turkami. Kolejny już raz imperium osmańskie powiększa swe terytorium kosztem świata chrześcijańskiego.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1471.1";
			Text = "Książę Walii nie żyje - został zabity na tym samym polu bitwy, z którego uciekła królowa Małgorzata ze swą rozbitą armią. Edward IV po raz kolejny zajmuje tron - gdzie jest król Henryk?";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1472.0";
			Text = "Umarł Henryk VI, niech żyje król Edward IV!";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1472.1";
			Text = "Podróżnicy ze Szkocji zapraszają szukających rozrywki do przyłączenia się do gry zwanej przez nich %\"Gowff%\". Potrzeba długiego kija, by uderzyć leżącą na trawie piłeczkę, którą trzeba trafić do dołka znajdującego się parę kilometrów dalej. Ha! Powodzenia, moi mili. Ta głupia rozrywka nigdy nie stanie się popularna.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1473.0";
			Text = "Augsburskiemu rodowi bankierów, Fuggerom, nadano herb. Teraz już nic nie stoi na przeszkodzie, żeby ich dynastia rosła w siłę.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1473.1";
			Text = "Sułtan Mehmed II pokonuje Turkmenów Ak Kojunłu i utrzymuje swą dominację w tym rejonie Arabii.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1474.0";
			Text = "W tym roku doża ogłosił wprowadzenie pierwszego weneckiego prawa patentowego, które natychmiast wchodzi w życie. Prawo to chroni inwencję pomysłowych i przecierających nowe szlaki rzemieślników.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1474.1";
			Text = "Po śmierci Henryka IV, króla Kastylii, wybucha wojna pomiędzy Izabelą a jej siostrą, Joanną.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1475.0";
			Text = "Po krótkich walkach król zawiera pokój z Francją - miejmy nadzieję, że będzie on trwały.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1475.1";
			Text = "Papież Sykstus IV uczcił Rok Święty, budując Kaplicę Sykstyńską i wznosząc most na Tybrze.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1476.0";
			Text = "Konfederacja Szwajcarska zadała Karolowi Śmiałemu dwa poważne ciosy w bitwach pod Grandson i Morat. Karol stracił w ten sposób wszelkie nadzieje na podbój ziem szwajcarskich.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1476.1";
			Text = "Wład Palownik ponownie zostaje księciem Wołoszczyzny - wielu Cyganów mówi o panującym tam strachu oraz o tym, że Wład pije krew ludzi, których wbija na pal. Turcy w końcu kładą kres jego okrucieństwu, a jego głowa zostaje posłana do Mehmeda II, ich przywódcy. Wygląda na to, że nie traktują wrogów dużo lepiej.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1477.0";
			Text = "Karol Śmiały, władca Burgundii, zostaje pokonany i ginie w bitwie pod Nancy. Jego córka, Maria, zostaje księżną, a kilka tygodni później jej niezadowoleni poddani zmuszają ją do podpisania dokumentu, dzięki któremu flandryjskie miasta odzyskują swe prawa.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1477.1";
			Text = "Maria, księżna Burgundii, poślubia w Gandawie cesarza Maksymiliana I - dzięki temu jej francuskie ziemie zostają przyłączone do Świętego Cesarstwa Rzymskiego.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1478.0";
			Text = "Konfederacja Szwajcarska sromotnie upokorzyła Mediolan. Podobno około sześciuset ludzi z pomocą miejscowych błyskawicznym atakiem odparło liczącą kilka tysięcy żołnierzy mediolańską armię.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1478.1";
			Text = "Wiadomości o poszukiwaczach przygód oraz ogromnych bogactwach za pomaganie królowej Kastylii i Aragonii pomagają usunąć ostatnich muzułmańskich władców z Iberii. Najemnicy są sowicie opłacani.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1479.0";
			Text = "Doża w całej swej mądrości podpisał tego roku z tureckim sułtanem traktat pokojowy. Doża chce okazać, jak bardzo pragnie pokoju, więc w geście przyjaźni postanowił sprezentować sułtanowi weneckie ziemie na wybrzeżu Grecji oraz miasta Lezha i Szkodra.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1479.1";
			Text = "Węgry i imperium osmańskie kontynuują wojnę pozycyjną - papież wyraża wobec swych biskupów troskę dotyczącą rozprzestrzeniania się.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1480.0";
			Text = "Turcy zajęli niezwykle ważne miasto portowe Otranto i dzięki temu zaczęli kontrolować dostęp do Adriatyku. W reakcji na to Neapol rozpoczął blokadę Otranto od strony lądu i w ten sposób otoczył Turków.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1480.1";
			Text = "Wielkie Księstwo Moskiewskie zmusza do odwrotu Mongołów ze Złotej Ordy, zajmując tereny nad Wołgą.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1481.0";
			Text = "Turcy opłakują śmierć Mehmeda II. Nowym sułtanem został Bajazyd II. Ludzie powiadają, że w geście dobrej woli chce on opuścić Otranto.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1481.1";
			Text = "Umarł sułtan turecki - miejmy nadzieję, że jego syn, Bajazyt II, jest głupcem.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1482.0";
			Text = "Iwan III uwalnia się spod zwierzchnictwa Tatarów i ogłasza niepodległość swych ziem.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1482.1";
			Text = "Ze smutkiem obwieszczamy, iż była królowa Anglii, nasza ukochana Małgorzata Andegaweńska, zmarła we Francji. Dzielnie walczyła bez pomocy męża o zachowanie korony angielskiej, ale w końcu jej duch został złamany. Niech spoczywa w pokoju.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1483.0";
			Text = "Pewien genueński marynarz wyruszył do Portugalii, chcąc wydobyć od króla pieniądze na podróż dookoła świata i odnalezienie drogi morskiej do Indii. Przecież nawet dziecko wie, że Ziemia jest płaska!";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1483.1";
			Text = "Tragedia - młody król Edward V został zamordowany, a jego miejsce zajął wuj Ryszard. Nie spodobało się to księciu Buckingham, który podniósł bunt.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1484.0";
			Text = "Pedro Arbues, inkwizytor hiszpański, zostaje zamordowany w trakcie modlitwy w katedrze w Saragossie.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1484.1";
			Text = "Papież Innocenty VIII zlecił arystokratom polowanie na heretyków i czarownice. Na miejscu czarownic leciałbym w te pędy do najbliższego kościoła.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1485.0";
			Text = "Chowajcie się do domów i zaryglujcie drzwi - w kraju szaleje nowa choroba zwana %\"potami angielskimi%\". Zbliża się plaga, módlcie się za swe dusze!";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1485.1";
			Text = "W Anglii trwa wojna domowa - Boże, ocal nas - król Ryszard III poległ w bitwie pod Bosworth z ręki Henryka Tudora, hrabiego Richmond.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1486.0";
			Text = "Henryk Tudor koronowany na króla Anglii - zostaje Henrykiem VII. Niech żyje król!";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1486.1";
			Text = "Zakup Młot na czarownice, podręcznik łowców czarownic wydany przez niemieckich inkwizytorów.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1487.0";
			Text = "Pretendent do tronu Anglii, Lambert Simnel, został pokonany pod Stoke Field przez armię lojalną królowi Henrykowi VII. Hurra!";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1487.1";
			Text = "Portugalski odkrywca, Diaz, dotarł do południowego krańca Afryki i podobno żegluje dalej, aż na nieznane wody, po których pływał kiedyś Aleksander Wielki.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1488.0";
			Text = "Jedenastoletnia Anna Bretońska zostaje księżną Bretanii, więc dzieci: nie zapomnijcie o chodzeniu do kościoła i pilnej nauce, bo dzięki ciężkiej pracy wy również możecie się stać arystokratami.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1488.1";
			Text = "Jakub IV zostaje nowym królem Szkocji. Kogo to obchodzi? Ha, ha, ha...";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1489.0";
			Text = "Królowa Cypru Katarzyna Cornaro abdykowała, przekazując władzę naszej ukochanej Republice Weneckiej. Dzięki temu szlachetnemu czynowi umocniła więź pomiędzy Wenecją a jej terenami na Cyprze.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1489.1";
			Text = "W Hiszpanii szaleje tyfus - to pierwszy jego przypadek w Europie. Miejmy nadzieję, że do nas nie dotrze.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1490.0";
			Text = "Chcesz zostać misjonarzem w Afryce? Utoczyłbyś krwi z serca dla zagubionych i dla złota? Kościół katolicki planuje misję w Kongo - chętni spotykają się dziś wieczorem w kościele.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1490.1";
			Text = "Umarł król Węgier, Maciej Korwin, pozostawiając kraj na pastwę Turków. Co się teraz stanie?";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1491.0";
			Text = "Król Francji, Karol VIII zmusza młodą Annę Bretońską do zerwania małżeństwa z Maksymilianem Habsburgiem - dzięki temu Francja może przyłączyć Bretanię do Korony francuskiej.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1491.1";
			Text = "Król Ferdynand i królowa Izabela z pomocą wielu najemników podbijają Grenadę i kończą osiemsetletnie mauretańskie rządy.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1492.0";
			Text = "Odkrywca Krzysztof Kolumb wypływa trzema statkami w podróż na zachód, by udowodnić, że tędy można dopłynąć na Daleki Wschód. Ha! Przecież wszyscy wiemy, że spadnie z krawędzi ziemi. Dziś wieczorem odbędzie się msza, na której ksiądz wyjaśni, dlaczego ziemia jest płaska i dlaczego ci odkrywcy-heretycy spadną w czeluście piekielne.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1492.1";
			Text = "Wszyscy Żydzi i czarnoskórzy zostają wydaleni z Hiszpanii - inkwizycja ciągle walczy z heretykami, którzy odmawiają przejścia na katolicyzm. Eee... biskup odprawi nabożeństwo, na którym wyjaśni, dlaczego tak się musi dziać.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1493.0";
			Text = "Genueńczyk Kolumb powrócił z podróży, którą odbył w imieniu korony hiszpańskiej. Ponoć na własne oczy widział odległe ziemie oraz bogactwa Ameryki i zajął je dla Hiszpanii.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1493.1";
			Text = "Hiszpańscy odkrywcy znajdują drogę z zachodu na Daleki Wschód, a Portugalczycy okrążają Afrykę, żeby dopłynąć tam z południa. Odkrywają Ocean Indyjski - wspaniałą drogę z południa, której zamierzał użyć Aleksander Wielki.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1494.0";
			Text = "Hiszpania i Portugalia zawierają traktat w Tordesillas - nowo odkryte lądy zostają między nie podzielone.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1494.1";
			Text = "Zastanawiacie się, dlaczego król Anglii nie wysyła podróżników, by odkrywali nowe lądy? Dzieje się tak dlatego, że jego zaufani ludzie wyjeżdżają do Hiszpanii i Portugalii, musicie więc mu pomóc - wyróbcie sobie reputację, ciężko pracujcie, a może i wy będziecie mogli odkrywać lądy dla królestwa.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1495.0";
			Text = "Chcąc powstrzymać bezwstydne zakusy króla Francji, utworzono dziś Ligę Wenecką pod przewodnictwem doży. Pozostali członkowie to Hiszpania, Mediolan, cesarz Maksymilian I oraz papież Aleksander VI. Oby doprowadzili Francję do upadku.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1495.1";
			Text = "W Portsmouth w Anglii powstaje pierwszy suchy dok - proszę, dobrzy ludzie, król przygotowuje flotę do odkrywania nowych miejsc...";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1496.0";
			Text = "Król Henryk VII pisze list do Johna Cabota i jego synów, upoważniając ich do odkrywania nowych lądów. W ratuszu możecie się zapisywać na przyszłe wyprawy.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1496.1";
			Text = "Włoski wynalazca zwany Leonardo da Vinci bez powodzenia próbował stworzyć maszynę latającą. Co ci ludzie sobie myślą? Że są ptakami? He, he - szaleńcy!";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1497.0";
			Text = "Iwan III, wielki książę moskiewski wprowadza nowy kodeks - większość Mongołów ma zostać mu podporządkowanych lub usuniętych z jego ziem. Słaba Rosja nabiera kształtów. Ordy Mongołów jeszcze przez dwie dekady nękają książęta rosyjskie.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1497.1";
			Text = "Henryk VII pokonuję armię buntowników pod wodzą Michaela an Gofa. John Cabot powrócił - podobno rzeczywiście odnalazł ląd po drugiej stronie Atlantyku.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1498.0";
			Text = "Portugalczykowi Vasco da Gamie udaje się opłynąć Afrykę.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1498.1";
			Text = "John Cabot nie powrócił z ostatniej wyprawy - to bardzo smutne wieści, ale sądzimy, że wszystkiemu winni są hiszpańscy piraci, którzy nie chcą konkurencji Anglików. Młodzi mężczyźni, zapisujcie się w ratuszu - już czas pokazać Hiszpanom, skąd pochodzą najwięksi odkrywcy.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1499.0";
			Text = "Żeglarz Vasco da Gama obładowany klejnotami powrócił z wyprawy do Indii i został przyjęty z honorami przez koronę portugalską.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1499.1";
			Text = "Pretendent do tronu, Perkin Warbeck, oraz Edward, hrabia Warwick, ostatni członek dynastii York, zostali straceni za zdradę.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1500.0";
			Text = "Poszukiwacz przygód, Portugalczyk Pedro Cabral odkrywa dla swojego króla nowy ląd i nazywa go Brazylią. Kochani podróżnicy, można odkryć mnóstwo nowych ziem, a król oferuje nagrody dla tych, którzy zrobią to dla Anglii.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1500.1";
			Text = "W katedrze odprawiona zostanie specjalna msza za Edmunda Tudora, księcia Somerset.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1501.0";
			Text = "Ismail pokonuje konfederację Ak Kojunłu i zakłada dynastię Safawidów w Persji. To szyici, a więc wrogowie Turków - miejmy nadzieję, że pomoże to w czymś chrześcijanom w Europie.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1501.1";
			Text = "Odbędzie się publiczna egzekucja dwojga arystokratów oskarżonych o czary i rozpowszechnianie plotek, że władze Kościoła nie mają prawa sprzedawać odpustów grzesznikom. Coś tu śmierdzi.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1502.0";
			Text = "Dowiedziałem się, że żywot Piera de Medici dobiegł końca. Okryty hańbą regent Florencji, który przez całe życie starał się naprawić wyrządzone przez siebie szkody, zginął w jednej z bitew w okolicy Neapolu.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1502.1";
			Text = "Rządzone przez Iwana III Wielkie Księstwo Moskiewskie bez powodzenia atakuje Litwę.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1503.0";
			Text = "Król Szkocji Jakub IV żeni się z Małgorzatą Tudor, córką Henryka VII. To może być dobra nowina albo... bardzo zła.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1503.1";
			Text = "W bitwie pod Cerignolą armia francuska zostaje pokonana przez Hiszpanów, którzy używają broni palnej i prochu strzelniczego.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1504.0";
			Text = "Dotarły dziś do nas smutne wieści. Długo się opierały, lecz teraz skończyło się ich szczęście. Księstwo Mediolanu i Królestwo Sycylii padły łupem korony hiszpańskiej.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1504.1";
			Text = "Stefan III Mołdawski walczy o niepodległość swego kraju przeciw Polakom i Turkom.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1505.0";
			Text = "Portugalczycy docierają do Kolombo na Sri Lance i wysyłają posłów do króla Kotte.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1505.1";
			Text = "Polski król Aleksander Jagiellończyk podpisuje ustawę, która zabrania chłopom opuszczania ziemi, do której zostali przypisani - w ten sposób ustanowiona zostaje pańszczyzna.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1506.0";
			Text = "Na chwałę Pana i jako znak dla całego świata chrześcijańskiego rozpoczęto rekonstrukcję Bazyliki św. Piotra w Rzymie. Niech ten pomnik chrześcijaństwa świeci światłem prawdziwej Wiary.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1506.1";
			Text = "Umiera król Polski Aleksander Jagiellończyk, który urodził się w 1461 roku.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1507.0";
			Text = "Kolejny triumf wspaniałych umiejętności weneckich rzemieślników. Maestro Orlando Gallo zdołał znacznie poprawić jakość luster weneckich, a przez to również ogromnie zwiększyć renomę przedmiotów wytwarzanych w tym mieście.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1507.1";
			Text = "Umiera francuski poeta Jean Molinet.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1508.0";
			Text = "Cóż za skandal! Usłyszałem, co następuje: podobno papież, cesarz, król Francji Ludwik XII oraz król Hiszpanii Ferdynand Katolicki utworzyli sojusz przeciwko naszej ukochanej Republice. Nazwali go";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1508.1";
			Text = "Utworzona przez papieża Juliusza II, cesarza Maksymiliana I i króla Ludwika XII Liga w Cambrai powstrzymuje ekspansję Republiki Weneckiej. Bogaci nie zawsze dostają to, czego pragną.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1509.0";
			Text = "Jakże gorzko smakuje porażka. Liga z Cambrai rozbiła nasze oddziały w okrutnej bitwie pod Agnadello. Serca nasze przepełnia smutek - módlmy się za poległych i ich rodziny.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1509.1";
			Text = "Henryk VIII bierze ślub z Katarzyną Aragońską - podobno gdy ksiądz powiedział %\"idźcie i rozmnażajcie się%\", na szaty króla spadły ptasie odchody.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1510.0";
			Text = "Utworzono Ligę Świętą, by powstrzymać chciwość korony francuskiej. Przymierze zawarli cesarz Maksymilian I, król angielski Henryk VIII, Wenecja oraz Szwajcaria.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1510.1";
			Text = "Z Wielkiego Księstwa Moskiewskiego powstaje Rosja, która rozpoczyna walkę z mongolskimi watażkami i ma na celu przejęcie pozostałych księstw.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1511.0";
			Text = "Tego roku ogłoszono wspaniałe zwycięstwo Ligi Świętej nad armią zachłannej korony francuskiej dowodzoną przez Pierre'a Bayarda.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1511.1";
			Text = "Margaret Beaufort zakłada St. John’s College na Uniwersytecie w Cambridge.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1512.0";
			Text = "Liga Święta po raz kolejny zadała Francuzom poważny cios w bitwie pod Novarą i wreszcie przepędziła ich z ziemi włoskiej. Niech Bóg ma Ligę w swej opiece!";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1512.1";
			Text = "Artysta zwany Michałem Aniołem kończy malowidła na sklepieniu Kaplicy Sykstyńskiej, którą sto lat temu zbudował kazał wybudować papież.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1513.0";
			Text = "Włoskie miasta-państwa rozpoczynają wojnę z Francuzami i innymi narodami.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1513.1";
			Text = "Juan Ponce de Leon odkrywa kolejne bogate ziemie w Ameryce - tym razem w grę wchodzi Floryda.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1514.0";
			Text = "Ludwik XII zawiera pokój z cesarzem Maksymilianem.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1514.1";
			Text = "Księżniczka Maria Tudor bierze ślub z Ludwikiem XII.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1515.0";
			Text = "Mający u boku liczną armię nowy król Francji Franciszek I najechał Włochy. Mimo bohaterskiego oporu obrońców Mediolanu udało mu się zdobyć miasto w bitwie pod Marignano.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1515.1";
			Text = "Turcy podbijają Kurdystan.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1516.0";
			Text = "Nowy turecki sułtan nie miał dość wojny, więc najechał również mameluków w Syrii. Niech muzułmanie walczą między sobą, w ten sposób zostawią w spokoju Półwysep Bałkański.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1516.1";
			Text = "Sułtanat mameluków odchodzi w niepamięć, pokonany przez podążających na południe Turków.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1517.0";
			Text = "Marcin Luter przybija swe 95 tez do drzwi kościoła zamkowego w Wittenberdze, żeby sprowokować debatę dotyczącą sprzedaży odpustów. Ten człowiek chyba oszalał albo skończyły mu się pieniądze na zakup boskiego przebaczenia.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1517.1";
			Text = "Wybuch epidemii %\"potów angielskich%\" - ludzie, idźcie do medyka lub księdza. Czarownice rzuciły na nas wszystkich klątwę.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1518.0";
			Text = "Słynny angielski żołnierz, Marmaduke Constable, zmarł w spokoju.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1518.1";
			Text = "Plaga tropikalnych mrówek ognistych dewastuje plony na Hispanioli.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1519.0";
			Text = "Dzięki rządom Karola I w Hiszpanii nigdy nie zachodzi słońce. Od czasów Karola Wielkiego żaden władca nie posiadał tak ogromnego królestwa.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1519.1";
			Text = "Karol I, król Hiszpanii, zostaje wybrany cesarzem rzymsko-niemieckim i włada ogromnym cesarstwem, w którego skład wchodzą, m.in. Hiszpania, Ameryka i Austria. Staje się najpotężniejszym człowiekiem na świecie. Mam tylko nadzieję, że nie przyjdzie mu chrapka na Anglię.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1520.0";
			Text = "Sulejman I przejął tron turecki po ojcu, Selimie - módlmy się, by okazał się słabym człowiekiem i by zatrzymał się w końcu ten napawający lękiem pochód muzułmanów.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1520.1";
			Text = "Henryk VIII i król Francji Franciszek I spotykają się na Polach Złotogłowia - miejmy nadzieję, że dojdą do porozumienia, które zapewni spokój...";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1521.0";
			Text = "Papież Leon X ekskomunikuje Marcina Lutra za heretyckie potępienie odpustów.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1521.1";
			Text = "Karol V ogłasza edykt wormacki - sądzony jest heretyk Marcin Luter. Po zgromadzeniu Luter znika, nie pozostawiając śladów, a papież i cesarz wyznaczają nagrodę za jego głowę.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1522.0";
			Text = "Zakon joannitów został zmuszony do oddania swych ziem na Rodos, gdyż nie jest dłużej w stanie opierać się ciągłym atakom Turków. Rycerze zakonni przenieśli się na Maltę.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1522.1";
			Text = "Ferdynand Magellan powrócił do Hiszpanii na statku %\"Victoria%\" - podobno opłynął całą kulę ziemską. Och, jakież on musiał przeżyć przygody!";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1523.0";
			Text = "Szwecja doprowadza do rozpadu unii kalmarskiej i staje się niezależna od Danii i Norwegii. Gustaw Waza przejmuje kontrolę nad Szwecją, ale to niebezpieczne czasy.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1523.1";
			Text = "Umiera angielski kompozytor William Cornysh, jednak jego muzyka przetrwa na zawsze.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1524.0";
			Text = "W Świętym Cesarstwie Rzymskim Narodu Niemieckiego wybuchła wojna. Buntują się chłopi, którzy wszędzie słyszą o Marcinie Lutrze i możliwych zmianach w Kościele katolickim.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1524.1";
			Text = "Bitwa nad Sesią - w ciągle trwających wojnach we Włoszech siły hiszpańskie pokonują Francuzów.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1525.0";
			Text = "W Szwajcarii rodzą się anabaptyści - Konrad Grebel, Felix Manz i wielu innych zostaje ochrzczonych w zwykłym domu.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1525.1";
			Text = "Marcin Luter bierze ślub z Katarzyną von Bora.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1526.0";
			Text = "Pomimo swej heroicznej odwagi król węgierski Ludwik II zginął w bitwie pod Mohaczem. Posuwający się naprzód Turcy zajęli już wschodnią i środkową część Węgier.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1526.1";
			Text = "Babur zakłada dynastię Wielkich Mogołów w Indiach mniej więcej w tym samym czasie, gdy portugalscy odkrywcy zaczynają handlować z krajami położonymi nad Oceanem Indyjskim.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1527.0";
			Text = "Rzym zostaje splądrowany przez siły cesarza Karola V - nie ustaje walka między Świętym Cesarstwem Rzymskim a Ligą Świętą z Cognac. Podobno papieżowi udaje się uciec, a Marcin Luter stwierdza, iż Chrystus rządzi w taki sposób, że cesarz, który prześladuje Lutra dla papieża zostaje zmuszony do zniszczenia papieża dla Lutra.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1527.1";
			Text = "Hiszpanie podbijają Gwatemalę i zakładają tam pierwsze miasto, Ciudad Vieja. Podążają dalej, bezwzględnie podbijają Kubę i Meksyk, atakują tubylców, łupią - walczą również z chcącymi zbadać te tereny załogami statków z innych krajów.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1528.0";
			Text = "Kolejna plaga %\"potów angielskich%\", i to nie tylko w Anglii, ale również w północnej Europie. Ej! Pocisz się! Odsuń się ode mnie!";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1528.1";
			Text = "Gustaw I zostaje koronowany na króla Szwecji.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1529.0";
			Text = "Turcy stanęli u bram Wiednia! Nienasycona bisurmańska armia najpierw przebrnęła przez węgierskie wsie, a teraz zagraża Wiedniowi.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1529.1";
			Text = "Król Henryk zwraca się do Thomasa Wolseya, arcybiskupa Yorku, o pomoc w uzyskaniu zgody na rozwód z Katarzyną Aragońską. Doradca króla zawodzi, więc król karze go, odbierając mu ziemie.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1530.0";
			Text = "Joannici z Rodos zostają pokonani przez Turków, kończąc w ten sposób swe parusetletnie panowanie rozpoczęte w czasach krucjat. Pokonani zakonnicy otrzymują ziemie na Malcie, jednak dni chwały już nie wrócą.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1530.1";
			Text = "Dowodzone przez Filiberta van Châlon oddziały hiszpańskie zdobywają Florencję. Medyceusze zostają przywróceni do władzy.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1531.0";
			Text = "Henryk VIII zostaje uznany głową Kościoła w Anglii i zmienia obowiązujące prawa, żeby umożliwić sobie rozwód z Katarzyną Aragońską, nawet jeśli ma to oznaczać odcięcie się od Rzymu.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1531.1";
			Text = "Lizbonę, stolicę Portugalii, dotyka potężne trzęsienie ziemi, pozostawiając po sobie tysiące ofiar.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1532.0";
			Text = "Zaciekłe walki pod Kőszeg uniemożliwiły Turkom ponowne przekroczenie węgierskiej granicy. Kiedy ci przeklęci poganie dadzą nam wreszcie spokój?";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1532.1";
			Text = "Po śmierci Wasyla III jego syn, Iwan IV, zostaje wielkim księciem moskiewskim w wieku trzech lat.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1533.0";
			Text = "Król Henryk VIII ponownie się ożenił, tym razem z Anną Boleyn. Chwała królowi! Niech żyje król i jego... eee... żony.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1533.1";
			Text = "Papież Klemens VII nakłada ekskomunikę na króla Henryka VIII.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1534.0";
			Text = "Nowy Testament w tłumaczeniu Marcina Lutra staje się coraz powszechniejszy, co sprawia, że zaczynają się pojawiać coraz to nowe przekłady w różnych językach przeznaczone dla ludu.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1534.1";
			Text = "Anabaptyści dochodzą do władzy w Münster i ogłaszają je Nowym Jeruzalem, skazując dysydentów na wygnanie.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1535.0";
			Text = "W tym tygodniu William Tyndale wprowadził do obiegu angielski przekład Biblii. Teraz mamy szansę zobaczyć na własne oczy, o czym przez te wszystkie lata mówili duchowni.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1535.1";
			Text = "Hiszpańscy podróżnicy zakładają Limę w Peru.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1536.0";
			Text = "Król Anglii rozkazał ściąć swą ukochaną drugą żonę, Annę Boleyn. Żeby poślubić tę kobietę, wdał się w konflikt z papieżem i założył własny kościół. Wynika z tego, że ładna buzia nie zawsze warta jest zachodu.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1536.1";
			Text = "Walia zostaje oficjalnie inkorporowana do Anglii.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1537.0";
			Text = "Uwaga, dobrzy ludzie! Przynoszę wieści o nowym warzywie sprowadzonym do nas przez hiszpańskich podróżników. Zwie się ono %\"ziemniakiem%\".";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1537.1";
			Text = "Religia luterańska rozprzestrzenia się na ziemiach niemieckich i zostaje wprowadzona w Danii i Norwegii przez króla Chrystiana III. Kościół katolicki nie jest z tego powodu nadmiernie szczęśliwy.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1538.0";
			Text = "Jan Zapolya zostaje wybrany na króla Węgier po zawarciu pokoju z Turkami. Węgry dzielą się na część kontrolowaną przez Turków na południu i przez króla na północy.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1538.1";
			Text = "W Santo Domingo powstaje pierwszy uniwersytet w Nowym Świecie.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1539.0";
			Text = "W Kościele katolickim zachodzą zmiany - począwszy od Niemiec, aż po Anglię, gdzie reformacja nabiera kształtu, kolejny raz dzieląc świat religii.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1539.1";
			Text = "Król Henryk VIII zobowiązuje się poślubić Annę z Kleve.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1540.0";
			Text = "Papież Paweł III zatwierdził prawa i obowiązki zakonu jezuitów zwanego Towarzystwem Jezusowym.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1540.1";
			Text = "W Hiszpanii powstaje Towarzystwo Jezusowe, zakon religijny Kościoła katolickiego. Jezuici skupiają się na wykształceniu oraz na działalności misyjnej w obronie praw człowieka. Rozpoczynają pracę w obu Amerykach, ale planują również podróż do Azji.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1541.0";
			Text = "Irlandzki parlament ogłasza Henryka VIII królem Irlandii.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1541.1";
			Text = "Hernando de Soto dociera do Rio de Espiritu Santo, czyli do rzeki, którą my w Anglii zwiemy Missisipi.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1542.0";
			Text = "Pomiędzy Francją a Karolem V, cesarzem rzymsko-niemieckim, wybucha wojna. Anglia staje po stronie cesarza, a Szkoci i Turcy pomagają Francji. Psubraty!";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1542.1";
			Text = "Katarzyna Howard, piąta żona Henryka VIII, zostaje stracona za cudzołóstwo.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1543.0";
			Text = "Wyprawiono huczne wesele, zacieśniając więzi między dwoma królestwami. Prawowity następca tronu Hiszpanii, Filip II, poślubił przepiękną infantkę Marię Manuelę Portugalską.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1543.1";
			Text = "Marcin Luter wydaje książkę O Żydach i ich kłamstwach - martwi go zwiększająca się liczba katolików przechodzących na judaizm.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1544.0";
			Text = "Cesarz Karol V najeżdża wschodnią Francję, a król Henryk II zajmuje Boulogne.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1544.1";
			Text = "Węgry zostają ponownie najechane przez Turków - tym razem psy bisurmańskie kradną klejnoty koronne.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1545.0";
			Text = "Szkockie wojska pokonują przeważające siły angielskie w bitwie pod Ancrum Moor.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1545.1";
			Text = "Uczestnicy soboru trydenckiego omawiają stan Kościoła, skutki reformacji i wyrażają obawy dotyczące wzrastającej popularności protestantyzmu.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1546.0";
			Text = "Anglia i Francja wreszcie podpisują traktat pokojowy - może wreszcie zakończy się rozlew krwi.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1546.1";
			Text = "Hiszpania podbija Jukatan i wraz z Portugalią stale odnosi sukcesy w obu Amerykach.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1547.0";
			Text = "Rebelia przeciw dożom Genui poniosła sromotną klęskę. Przywódca buntowników, Giovanni Luigi de Fieschi, zginął, spadłszy z nabrzeża w pełnej zbroi. Wraz z jego utonięciem zakończyło się powstanie.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1547.1";
			Text = "Umiera Henryk VIII - cóż za tragedia! Wkrótce koronacja Edwarda VI - niech żyje król!";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1548.0";
			Text = "Iwan IV Groźny zostaje pierwszym carem Rosji, jednocząc książęta i ziemie rosyjskie. Nazwa %\"car%\" ma oznaczać %\"cezara%\" i spadkobiercę cesarstwa rzymskiego, a Moskwa ma być trzecim Rzymem. W 1469 Iwan III wziął za żonę Zofię, bratanicę ostatniego cesarza bizantyjskiego.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1548.1";
			Text = "Anglia coraz bardziej oddala się od Kościoła katolickiego - wiele symboli jest niszczonych w świątyniach, a ludowi rozdaje się protestanckie książki. Arystokracja żywi coraz cieplejsze uczucia wobec ruchu protestanckiego.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1549.0";
			Text = "Salvador zostaje pierwszą stolicą Brazylii, stanowiącej część potężnego terytorium będącego własnością Portugalii.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1549.1";
			Text = "Papież Paweł III zapoczątkowuje kontrreformację, która ma zmienić Kościół katolicki tak, by sprzeciwiająca się tradycji katolickiej reformacja straciła na popularności. Ruch ten nawołuje również do zwiększonych prześladowań osób, które posuną się za daleko w krytyce Kościoła katolickiego.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1550.0";
			Text = "Nostradamus pisze swój pierwszy almanach. Pracuje również we Francji i w cudowny sposób leczy ludzi. Niektórzy nazywają go nekromantą, bo wydaje się, że przywraca martwych do życia.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1550.1";
			Text = "Gustaw Waza, król szwedzki, buduje Helsinki. Szwecja staje się potęgą w Skandynawii, zagrażając pozycji Danii i Norwegii.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1551.0";
			Text = "Na życzenie jezuitów w Rzymie założono Uniwersytet Papieski, by sławić chrześcijaństwo i - rzecz jasna - zgłębiać jego mądrość.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1551.1";
			Text = "Doszło do piątego wybuchu epidemii %\"potów angielskich%\". *kaszel* Ojej, trzeba mi do kościoła.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1552.0";
			Text = "Modlitewnik Kościoła anglikańskiego zostaje wprowadzony przez akt uniformizmu.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1552.1";
			Text = "Francuski król, Henryk II, najeżdża Lotaryngię i zdobywa kilka miast będących we władaniu cesarza.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1553.0";
			Text = "Edward nie żyje! Mój Boże, miał jedynie szesnaście lat - teraz nie ma żadnego męskiego następcy tronu. Joanna Grey zostaje ogłoszona pierwszą królową Anglii.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1553.1";
			Text = "Maria Tudor zostaje ogłoszona prawowitą królową Anglii, a Joanna Grey z chęcią ustępuje. Eee... niech żyje królowa!";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1554.0";
			Text = "Joanna Grey zostaje ścięta za zdradę, gdyż w zeszłym roku usiłowała objąć tron. Smutny los spotkał tę biedną kobietę i jej męża.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1554.1";
			Text = "Królowa Maria Tudor wychodzi za Filipa, króla Neapolu, syna cesarza Karola V.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1555.0";
			Text = "Powiadają, iż korona brytyjska rozpoczęła budowę wielkiej floty. Zamiary jej nie są nam znane, gdyż przecież nigdy nie osiągnie ni rozmiaru, ni potęgi armady hiszpańskiej.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1555.1";
			Text = "Królowa Maria Tudor nawołuje Anglię do powrotu do katolicyzmu, próbując wykorzenić jak najwięcej skutków reformacji. W tym miesiącu na stosie spłonęło trzech prominentnych biskupów.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1556.0";
			Text = "Karol V abdykuje, a koronę hiszpańską przejmuje po nim jego syn Filip II, mąż angielskiej królowej Marii Tudor. Brat Karola, Ferdynand, otrzymuje koronę cesarską.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1556.1";
			Text = "Z Dalekiego Wschodu przybywają podróżnicy, przynosząc wieści o potężnym trzęsieniu ziemi, które zabiło osiemset tysięcy ludzi i o wojnach pomiędzy Mogołami i ludami Indii.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1557.0";
			Text = "Portugalscy kupcy zdołali ponoć założyć stałą placówkę handlową w odległych Chinach. W ten sposób otwiera się kolejna z dróg dla handlu wspaniałymi chińskimi wyrobami.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1557.1";
			Text = "Armia hiszpańska pokonuję armię francuską, ale król Filip nie idzie za ciosem i wycofuje się do Niderlandów.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1558.0";
			Text = "Maria I Stuart, królowa Szkocji, wychodzi za Franciszka II, króla Francji, jednocząc siły przeciw Anglii i Hiszpanii.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1558.1";
			Text = "Zmarła królowa Maria Tudor, a jej przyrodnia siostra Elżbieta niedługo zgłosi swe roszczenia do tronu. Och, czyż to nie wspaniały dzień, że Krwawa Maria nie żyje? Radujmy się, że będziemy mieć królową, która nie sprzeda nas Hiszpanii ani Rzymowi.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1559.0";
			Text = "Elżbieta zostaje koronowana na królową Anglii, ale odmawia zamążpójścia nawet królowi Hiszpanii Filipowi II. Ustanawia Kościół anglikański i rozpoczyna bezlitosne prześladowania katolików.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1559.1";
			Text = "Marynarze przybywając z Nowego Świata - Ameryki - przywożą arystokratom produkt zwany tytoniem. Lud nie może sobie na niego pozwolić... zresztą i tak strasznie śmierdzi.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1560.0";
			Text = "Odtąd już po wsze czasy Madryt, najpiękniejsze z hiszpańskich miast, będzie siedzibą rodu królewskiego.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1560.1";
			Text = "Kapitan Hak wzywa wszystkich silnych mężczyzn, by przyłączyli się do jego wyprawy do Nowego Świata. Czekają niesamowite bogactwa, a jeśli Hiszpanie i Francuzi nie będą grać uczciwie, Anglicy też pozbędą się skrupułów. Zaczęła się era piratów!";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1561.0";
			Text = "Maria I Stuart nie może przejechać przez Anglię, ale prześladowani w opanowanej przez Hiszpanów Flandrii kalwiniści znajdują tu bezpieczną przystań.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1561.1";
			Text = "Filip II ustanawia Madryt stolicą Hiszpanii, a Hiszpan Ruy Lopez unowocześnia techniki gry w szachy.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1562.0";
			Text = "John Hawkins wypływa do Gwinei i Indii Zachodnich, by handlować niewolnikami - twierdzi, że może to przynieść spore zyski.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1562.1";
			Text = "Hugenoci desperacko starają się przeciwstawić odwetowi Francuzów - Anglicy przybywają z odsieczą.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1563.0";
			Text = "Po raz kolejny we Francji wybuchła otwarta wojna między hugenotami a katolikami. Brat walczy przeciw bratu. Ulice spływają krwią w imię Boga.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1563.1";
			Text = "Sobór Trydencki oficjalnie dobiega końca.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1564.0";
			Text = "Mam nadzieję, że wszystkim podoba się nasza swoboda - w Neapolu pocałunek w miejscu publicznym może zostać ukarany śmiercią. Stąd też wzięła się nazwa %\"pocałunek śmierci%\".";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1564.1";
			Text = "Bierzcie ołówki - alchemik i cieśla połączyli siły, żeby dać nam te wspaniałe przyrządy do pisania. Tanie i łatwe do wykonania - tylko nie pozwólcie dzieciom wkładać ich do buzi, fuj!";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1565.0";
			Text = "Hiszpańscy konkwistadorzy przepływają Pacyfik i lądują na Filipinach, żeby ustanowić tam kolonię i krzewić religię katolicką.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1565.1";
			Text = "Kawalerowie maltańscy, z których wielu przybyło jeszcze z Rodos, wytrzymują kolejne tureckie oblężenie.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1566.0";
			Text = "Oznajmiając białym dymem, kardynałowie ogłosili %\"Habemus papam%\". Nowym Przedstawicielem Chrystusa został Pius V. Niech Bóg błogosławi Papieża.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1566.1";
			Text = "Po śmierci Sulejmana Wspaniałego sułtanem imperium osmańskiego zostaje Selim II.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1567.0";
			Text = "Ledwo kilka miesięcy po tym, jak mąż Marii I Stuart zostaje zamordowany, ona sama przegrywa bitwę przeciw szkockim arystokratom i abdykuje na rzecz Jakuba VI.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1567.1";
			Text = "Hugenoci rozpoczynają kolejną wojnę we Francji.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1568.0";
			Text = "Maria I Stuart ucieka z niewoli, ale przegrywa bitwę z wojskami szkockimi, więc kieruje się do Anglii, gdzie zostaje aresztowana na rozkaz Elżbiety I.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1568.1";
			Text = "Turkom nie udaje się pokonać Austriaków i godzą się na zapłacenie trybutu za zniszczenia wojenne.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1569.0";
			Text = "Dzień wstydu i hańby dla naszej dumnej Republiki. Mimo heroicznego oporu, jaki stawiły nasze wojska, posiadłości na Cyprze wpadły w ręce Turków.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1569.1";
			Text = "Polska łączy się z Wielkim Księstwem Litewskim dzięki unii lubelskiej.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1570.0";
			Text = "Regent James Stewart zostaje zamordowany, co zapoczątkowuje w Szkocji wojnę domową.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1570.1";
			Text = "Papież Pius V nakłada ekskomunikę na Elżbietę I bullą Regnans in Excelsis. Dalej pogarszają się stosunki między Anglią, Hiszpanią a Rzymem.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1571.0";
			Text = "Nadchodzi czas wielkich czynów! Ogłoszono, iż Hiszpania, Wenecja, Sabaudia, Genua i kawalerowie maltańscy zjednoczyli się pod papieskim sztandarem w Lidze Świętej przeciw Turkom.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1571.1";
			Text = "Wiktoria! W bitwie morskiej, jakiej świat jeszcze nie widział, wojska Ligi Świętej pobiły Turków i posłały niewiernych oraz ich okręty na samo dno morza.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1572.0";
			Text = "Holenderscy buntownicy zwani gezami zapoczątkowują wojnę domową przeciw katolikom i władzy hiszpańskiej.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1572.1";
			Text = "Małżeństwo między przywódcą hugenotów, Henrykiem IV, królem Nawarry i księżną Małgorzatą de Valois, siostrą francuskiego króla Karola IX, ma pomóc w pogodzeniu protestantów i katolików.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1573.0";
			Text = "W dniu dzisiejszym obwieszczono, iż celem zachowania pokoju, nasza Republika zawarła odrębne porozumienie z Turkami. Cenę jednakże ponosimy wielką, gdyż musimy oddać Cypr.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1573.1";
			Text = "Wiadomości z dalekich krajów: Oda Nobunaga wypędza z Kioto szoguna Ashikagę, kończąc okres Muromachi.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1574.0";
			Text = "We Francji rozpoczyna się piąta wojna religijna przeciw hugenotom - nie ma tam pokoju między wyznaniami.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1574.1";
			Text = "Murad III zajmuje miejsce Selima II na czele imperium osmańskiego. Umiera król Francji, a jego następcą zostaje sprowadzony z Polski Henryk III Walezy, w którego imieniu rządzi matka - regentka.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1575.0";
			Text = "Kryzys! Kufry hiszpańskiego monarchy świecą pustkami niczym sakiewka żebraka. Żaden zdrowy na umyśle kupiec nie udzieli hiszpańskiemu pawiowi pożyczki. W ten oto sposób wiele banków popadło w ruinę.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1575.1";
			Text = "Stefan Batory zostaje wybrany królem Polski.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1576.0";
			Text = "Angielski żeglarz, Martin Frobisher, poszukuje Przejścia Północno-Zachodniego. Powstaje Kompania Kataju, by wspierać jego poszukiwania złota w Nowym Świecie.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1576.1";
			Text = "Rudolf II zostaje Cesarzem Świętego Cesarstwa Rzymskiego Narodu Niemieckiego po śmierci Maksymiliana II.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1577.0";
			Text = "Sir Francis Drake wyrusza w podróż dokoła świata z Plymouth w Anglii.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1577.1";
			Text = "Dzięki unii brukselskiej wszystkie prowincje niderlandzkie zjednoczyły się w otwartym buncie przeciw Hiszpanom.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1578.0";
			Text = "Martin Frobisher wraz z członkami swej wyprawy obchodzą w Ameryce Północnej Święto Dziękczynienia.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1578.1";
			Text = "Marokańczycy pokonują armię portugalską i zabijają króla Sebastiana I, pozbawiając kraj następcy.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1579.0";
			Text = "Francis Drake ląduje w Kalifornii i zajmuje w imieniu Anglii ziemie na północ od Nowej Hiszpanii, od Atlantyku do Pacyfiku. Daje to początek kolejnym napięciom pomiędzy Hiszpanią, Anglią a Francją.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1579.1";
			Text = "Unia utrechcka jednoczy pod Wilhelmem Orańskim północne prowincje Niderlandów przeciw królowi Hiszpanii.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1580.0";
			Text = "Aby ponownie napełnić skarbiec, hiszpański król postanowił do cna oskubać swych poddanych. Wygląda na to, iż zapasy srebra w Nowym Świecie nie są tak obfite, jak się spodziewano.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1580.1";
			Text = "Ataman kozacki Jerman Timofiejewicz oraz kilka tysięcy jego ludzi rozpoczynają podbój Syberii dla Rosji.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1581.0";
			Text = "Parlament zakazuje katolicyzmu.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1581.1";
			Text = "Francis Drake powraca z podróży dokoła świata i otrzymuje szlachectwo od Elżbiety I.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1582.0";
			Text = "Papież Grzegorz XIII wprowadza kalendarz gregoriański.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1582.1";
			Text = "Północna część Niderlandów ogłasza niepodległość i odcinając się od Hiszpanii, zostaje Republiką Zjednoczonych Prowincji.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1583.0";
			Text = "W kościele odbywa się uroczystość - William Szekspir i Anna Hathaway biorą ślub.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1583.1";
			Text = "Humphrey Gilbert zakłada osadę angielską w Nowej Fundlandii, w Ameryce Północnej.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1584.0";
			Text = "Policzono wszystkie gwiazdy na niebie. Uczony Tycho Brahe poświęcił temu zagadnieniu lata badań i w swej najnowszej księdze wyliczył ich ponad tysiąc.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1584.1";
			Text = "Fiodor I, syn Iwana IV Groźnego, zostaje carem Rosji. Na Syberii powstają nowe osiedla.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1585.0";
			Text = "We Francji wybuchają wojny religijne, a Hiszpania rozpoczyna ofensywę przeciw separatystom w Niderlandach. Anglia przyłącza się do wojny osiemdziesięcioletniej.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1585.1";
			Text = "Lubisz łakocie? Awanturnicy i kupcy sprowadzają do Europy czekoladę. Nie zapomnij sakiewki - kosztuje krocie!";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1586.0";
			Text = "Francis Drake łupi Santo Domingo - nie tylko Hiszpanie i Francuzi zajmują się piractwem.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1586.1";
			Text = "Thomas Cavendish, angielski odkrywca, wyrusza w podróż dokoła świata.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1587.0";
			Text = "Maria I Stuart, zamieszana w spisek mający na celu morderstwo królowej Anglii, zostaje stracona w zamku Fotheringhay.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1587.1";
			Text = "Angielscy osadnicy przybywają na wyspę Roanoke i ponownie zakładają na niej kolonię.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1588.0";
			Text = "Hiszpańska flota straszliwie upokorzona! Największa z istniejących flot doznała porażki w bitwie z o wiele mniejszymi siłami dowodzonymi przez angielskich piratów.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1588.1";
			Text = "Hiszpańska Armada składająca się ze stu trzydziestu statków płynie na północ, by walczyć z Anglią. Po kilku bitwach morskich Hiszpanie zarządzają odwrót, tracąc po drodze znaczną część swej floty. Anglia zostaje nowym władcą mórz.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1589.0";
			Text = "Liga Katolicka rządzi w Paryżu po zabójstwie króla Henryka III. Korona jest do wzięcia, a Francja jest w całkowitym chaosie.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1589.1";
			Text = "Henryk IV, król Francji, bezskutecznie usiłuje odebrać Lidze Katolickiej Paryż, wojska hiszpańskie wyruszają na pomoc katolikom.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1590.0";
			Text = "Anna, księżniczka duńska zostaje koronowana na królową Szkocji.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1590.1";
			Text = "Urban VII umiera po zaledwie trzymiesięcznym pontyfikacie, a zastępuje go Grzegorz XIV.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1591.0";
			Text = "Król Hiszpanii stłumił w zarodku knowania aragońskiej szlachty. Aragończykom cofnięto wszelkie przywileje. Hiszpania to istna beczka prochu.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1591.1";
			Text = "Odnaleziona osada Roanoke okazuje się być opuszczona.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1592.0";
			Text = "Ubiegłoroczna śmierć papieża Innocentego IX w zeszłym roku pozostawia katolików bez duchowego przywódcy. Po jakimś czasie kardynałowie wybierają na papieża Klemensa VIII.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1592.1";
			Text = "Henryk IV ogłasza chęć podążenia za nauką Kościoła katolickiego i odsunięcia się od protestantyzmu, by zostać królem Francji.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1593.0";
			Text = "Na Węgrzech wybucha %\"długa wojna%\" pomiędzy krajami europejskimi a imperium osmańskim.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1593.1";
			Text = "Dramatopisarz Christopher Marlowe zostaje pojmany za wygłaszanie herezji i zamordowany.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1594.0";
			Text = "Podnieście wrzawę, dobrzy ludzie! Dziś weneckie banki oznajmiły, iż żaden z nich nie jest w stanie wypłacić pieniędzy. Wśród kupców wybuchła panika, wielu z rozpaczy rzuciło się w wody laguny.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1594.1";
			Text = "Nowy król, Henryk IV, po raz pierwszy bez oporu wkracza do Paryża i jednoczy Francję.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1595.0";
			Text = "Zostaje wystawiona nowa sztuka Williama Szekspira, Romeo i Julia.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1595.1";
			Text = "Habsburgowie wraz z sojusznikami pokonują Turków na Węgrzech i wypierają ich z tamtych terenów. Umiera Murad III, a sułtanem zostaje Mehmed III.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1596.0";
			Text = "W Nagasaki w Japonii dochodzi do ukrzyżowań katolików po tym, jak zakazane zostają wszelkie formy chrześcijaństwa.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1596.1";
			Text = "Hiszpania zajmuje francuskie Calais.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1597.0";
			Text = "Po raz kolejny zamiary Hiszpanów wobec Anglii obróciły się wniwecz. Sztorm zaskoczył ich okręty w cieśninie La Manche i zdruzgotana flota musiała zawrócić, nie osiągnąwszy niczego.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1597.1";
			Text = "Wystawiona zostaje pierwsza opera, zwiastując pojawienie się nowych form sztuki muzycznej i scenicznej.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1598.0";
			Text = "Traktat pokojowy w Vervins kończy wojnę między Francją i Hiszpanią.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1598.1";
			Text = "Angielski parlament uchwala ustawę, która zezwala na przewożenie skazańców do kolonii.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1599.0";
			Text = "Po raz pierwszy wystawiony zostaje Juliusz Cezar autorstwa Szekspira, który pisze również Hamleta. W Europie zaczyna rozkwitać sztuka.";
		},
		LocDirectEntry
		{
			Key = "$towncrier.1599.1";
			Text = "Siły irlandzkie wygrywają pierwszą bitwę pod Curlew Pass - powstanie skierowane przeciw okupacji protestanckich Anglików rośnie w siłę. Anglia jest na najlepszej drodze, by siłą osiągnąć dominację na świecie, prześciga nawet swego katolickiego rywala, Hiszpanię.";
		},
		LocDirectEntry
		{
			Key = "$action.DecommissionBuilding";
			Text = "Zburz budynek";
		},
		LocDirectEntry
		{
			Key = "$action.DecommissionBuildingDesc";
			Text = "With this action you can tear down your building. All employees will be fired and you lose all items in the storage of the building. But you get back part of the building value plus part of the value of all building upgrades.";
